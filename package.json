    "circom_runtime": "github:l-adic/circom_runtime#920bcb1"
  },
  "overrides": {
    "circom_runtime": "github:l-adic/circom_runtime#920bcb1"
  },
  "parcel": {
    "aliases": {
      "@bjorn3/browser_wasi_shim": "./node_modules/@bjorn3/browser_wasi_shim/src",
      "snarkjs": "./node_modules/snarkjs/build/main.cjs"
    }
  }
