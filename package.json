  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "mkdirp": "^0.5.1",
    "parcel": "^2.7.0",
    "purescript": "^0.15.15",
    "purescript-psa": "^0.8.2",
    "purs-tidy": "^0.10.0",
    "spago": "^0.21.0",
    "solc": "v0.8.15"
  },
  "dependencies": {
    "@bjorn3/browser_wasi_shim": "^0.3.0",
    "ethjs-provider-http": "^0.1.6",
    "keccak": "^1.0.2",
    "rlp": "^2.0.0",
    "secp256k1": "^3.0.1",
    "snarkjs": "github:l-adic/snarkjs#actually-use-wasm-options"
  },
  "overrides": {
    "circom_runtime": "github:l-adic/circom_runtime#wasi-import-options"
